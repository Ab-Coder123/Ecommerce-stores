{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productImg": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    },
    "Product": {
      "type": "string",
      "required": true
    },
    "Price": {
      "type": "decimal",
      "required": true
    },
    "Description": {
      "type": "string",
      "required": true
    },
    "Rating": {
      "type": "decimal",
      "required": true
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ],
      "required": true
    }
  }
}
